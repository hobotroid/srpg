public class CustomEvent extends TimerEvent
{
   public static const I_DID_SOMETHING:String = "iDidSomething";

   private var _imageToLoad:String;
 
   public function CustomEvent(type:String,	 imageToLoad:String):void {
       super(type);
       _imageToLoad = imageToLoad;
   {

   public function get imageToLoad():String
   {
       return _imageToLoad;
   }
}